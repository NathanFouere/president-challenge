{
  "name": "@president-challenge/server",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "license": "UNLICENSED",
  "scripts": {
    "start": "node build/bin/server.js",
    "build": "node ace build",
    "dev": "node ace serve --hmr",
    "test": "node ace test",
    "coverage": "c8 --reporter=lcov node ace test",
    "lint:fix": "eslint .",
    "lint:check": "eslint . --fix --quiet",
    "format": "prettier --write .",
    "typecheck": "tsc --noEmit",
    "startup-start": "node build/ace migration:run && node build/bin/server.js"
  },
  "files": [
    "build",
    ".env",
    "package.json"
  ],
  "imports": {
    "#common/*": "./app/common/*.js",
    "#budget/*": "./app/budget/*.js",
    "#law/*": "./app/law/*.js",
    "#user/*": "./app/user/*.js",
    "#tax/*": "./app/tax/*.js",
    "#social-class/*": "./app/social-class/*.js",
    "#product/*": "./app/product/*.js",
    "#happiness-modifier/*": "./app/happiness-modifier/*.js",
    "#sector/*": "./app/sector/*.js",
    "#election/*": "./app/election/*.js",
    "#state/*": "./app/state/*.js",
    "#political-party/*": "./app/political-party/*.js",
    "#legislature/*": "./app/legislature/*.js",
    "#licensed-file/*": "./app/licensed-file/*.js",
    "#game/*": "./app/game/*.js",
    "#event/*": "./app/event/*.js",
    "#controllers/*": "./app/controllers/*.js",
    "#exceptions/*": "./app/exceptions/*.js",
    "#models/*": "./app/models/*.js",
    "#builders/*": "./app/builders/*.js",
    "#queries/*": "./app/queries/*.js",
    "#mails/*": "./app/mails/*.js",
    "#services/*": "./app/services/*.js",
    "#listeners/*": "./app/listeners/*.js",
    "#events/*": "./app/events/*.js",
    "#middleware/*": "./app/middleware/*.js",
    "#validators/*": "./app/validators/*.js",
    "#providers/*": "./providers/*.js",
    "#policies/*": "./app/policies/*.js",
    "#abilities/*": "./app/abilities/*.js",
    "#game-config/*": "./game-config/*",
    "#database/*": "./database/*.js",
    "#tests/*": "./tests/*.js",
    "#start/*": "./start/*.js",
    "#config/*": "./config/*.js",
    "#commands/*": "./commands/*.js"
  },
  "devDependencies": {
    "@adonisjs/assembler": "^7.8.2",
    "@adonisjs/eslint-config": "^2.0.0-beta.6",
    "@adonisjs/prettier-config": "^1.4.0",
    "@adonisjs/tsconfig": "^1.4.0",
    "@japa/assert": "^3.0.0",
    "@japa/plugin-adonisjs": "^3.0.1",
    "@japa/runner": "^3.1.4",
    "@swc/core": "1.7.26",
    "@testcontainers/postgresql": "^10.16.0",
    "@types/luxon": "^3.4.2",
    "@types/node": "^22.7.5",
    "c8": "^10.1.3",
    "eslint": "^9.12.0",
    "hot-hook": "^0.3.1",
    "prettier": "^3.3.3",
    "ts-node-maintained": "^10.9.4",
    "typescript": "~5.6"
  },
  "dependencies": {
    "@adminjs/adonis": "^1.1.1",
    "@adminjs/relations": "^1.1.2",
    "@adonisjs/auth": "^9.2.3",
    "@adonisjs/core": "^6.14.1",
    "@adonisjs/cors": "^2.2.1",
    "@adonisjs/drive": "^3.2.0",
    "@adonisjs/lucid": "^21.3.0",
    "@adonisjs/session": "^7.5.0",
    "@adonisjs/shield": "^8.1.1",
    "@adonisjs/static": "^1.1.1",
    "@president-challenge/shared": "workspace:*",
    "@vinejs/vine": "^2.1.0",
    "adminjs": "^7.8.15",
    "edge.js": "^6.2.0",
    "luxon": "^3.5.0",
    "pg": "^8.13.1",
    "pino-pretty": "^11.2.2",
    "reflect-metadata": "^0.2.2"
  },
  "hotHook": {
    "boundaries": [
      "./app/controllers/**/*.ts",
      "./app/middleware/*.ts"
    ]
  },
  "overrides": {
    "strtok3": "8.0.1"
  },
  "resolutions": {
    "strtok3": "8.0.1"
  },
  "pnpm": {
    "overrides": {
      "strtok3": "8.0.1"
    }
  },
  "prettier": "@adonisjs/prettier-config"
}
